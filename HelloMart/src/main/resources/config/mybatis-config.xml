<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    
    <!-- resultType으로 맵으로 받아올 때, null 값을 가지는 컬럼은 맵에 키가 생성되지 않는 에러 -->
    <settings>
 	   <setting name="callSettersOnNulls" value="true"/>
    </settings>
     		
 	<typeAliases>
		<typeAlias type="com.hellomart.dto.Account" alias="account"/> 
		<typeAlias type="com.hellomart.dto.QABoard" alias="qaboard"/>
		<typeAlias type="com.hellomart.dto.ProductList" alias="productList"/>		
    </typeAliases>		
 
</configuration>
