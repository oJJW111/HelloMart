<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hellomart.dao.SellerDAO">

	<select id="getSellerProductList" resultType="productList" parameterType="String">
		SELECT * FROM ProductList WHERE RegisterID = #{id}
	</select>
	
	<select id="reviewCount" resultType="Integer" parameterType="Integer">
		SELECT COUNT(*) FROM Review WHERE No = #{no}
	</select>	
</mapper>