<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hellomart.dao.ProductListDAO">
	<select id="getMainList" parameterType="String">
		select * from productlist where maincategory = #{mainCategory}
	</select>
	
	<select id="getSmallList" parameterType="String">
		select * from productlist where smallcategory = #{smallCategory}
	</select>
	
	<parameterMap type="" id="detial">
	
	</parameterMap>
	
	<select id="getDetailList" parameterMap="detail">
	
	</select>
</mapper>