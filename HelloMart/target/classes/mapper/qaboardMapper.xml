<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hellomart.dao.QABoardDAO">

	<select id="listQABoard" resultType="QABoard">
		SELECT * FROM QABoard
	</select>
	
	<insert id="insertQABoard">
		INSERT INTO
		
		Account (Idx, Subject, Content, Id, ReRef,
		ReLev, ReSeq, Count, Date)
		
		VALUES (#{Idx}, #{Subject}, #{Content}, #{Id}, #{ReRef},
		#{ReLev}, #{ReSeq}, #{Count}, #{Date});
	</insert>
	
	<delete id="deleteAccount">
		DELETE FROM QABoard WHERE Idx = #{Idx}
	</delete>
	
	<select id="getCount">
		SELECT COUNT(*) FROM QABaord
	</select>
	
	
</mapper>